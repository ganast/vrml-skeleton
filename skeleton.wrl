																							]
																						}
																					]
																				}
																			]
																		}
																	]
																}
															]
														}
													]
												}

												DEF	shoulder-right Transform	{

													translation	-0.375 0 0

													children [

														DEF	shoulder-right-sh Shape {

															appearance Appearance {
																material USE joint-mat
															}

